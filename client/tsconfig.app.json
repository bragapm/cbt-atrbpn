{
  "compilerOptions": {
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "baseUrl": ".",
    "module": "ES2020",
    "moduleResolution": "bundler",
    "target": "ES2020", // Add this line
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node", "vite/client"]
  },
  "include": ["src"]
}
